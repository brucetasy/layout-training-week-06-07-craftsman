                ul.list-inline.c-navbarNavMenu.mb-0
                  mixin NavMenuLink(url, name)
                    li.list-inline-item.c-navbarNavMenu__item.mr-xl-6
                      if (path === url)
                        a.c-navbarLink.active.font-weight-bold(href='#{url}')
